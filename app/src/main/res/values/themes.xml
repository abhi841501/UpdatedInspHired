<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/skyBlue</item>
        <item name="colorPrimaryVariant">@color/darkRed</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="MyratingBar"  parent="android:style/Widget.Material.RatingBar">
        <!--none selected..-->
        <item name="colorControlNormal">@color/orange</item>
        <!--selected..-->
        <item name="colorControlActivated">@color/orange</item>
    </style>
    <style name="Theme.InspHiredApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.InspHiredApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.InspHiredApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="MyDatePicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:headerBackground">@color/skyBlue</item>
        <item name="android:numbersTextColor">@color/black</item>
        <item name="android:numbersSelectorColor">@color/white</item>
        <item name="colorAccent">@color/skyBlue</item>
    </style>

    <style name="MyTimePicker" parent="android:Widget.Material.TimePicker">
        <item name="android:headerBackground">@color/skyBlue</item>
        <item name="android:numbersSelectorColor">@color/skyBlue</item>
        <item name="android:colorAccent">@color/skyBlue</item>
        <item name="android:numbersTextColor">@color/black</item>

    </style>
</resources>